{
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "ES5",
    "module": "esnext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/*.ts", "rollup.config.ts"]
}
